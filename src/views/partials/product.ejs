<div class="conte">
  <div class="card iron-man">
<a href="/productos/detail/<%= products.id%>"><img src="/img/<%= products.image %>"/></a>
  </div>
  <div class="informacion">
    <h1><%= products.name %></h1>
  </div>
  <div class="precio">
    <div class="box-precio">
        <p id="antes">Antes $<%= products.price %></p>
        <p>Ahora $<%= products.price - products.descuento %></p>
        <p>Descuento del <%= Math.round(products.descuento * 100 / products.price) %>%</p>
    </div>
    <a href="#"><i class="fas fa-shopping-bag"></i></a>
  </div>
</div>